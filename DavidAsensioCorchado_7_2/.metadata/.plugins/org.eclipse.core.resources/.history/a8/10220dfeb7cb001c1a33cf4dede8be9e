package Ejercicio1;

public class empleado {
	
	
	private String dni;
	private String nombre;
	private String apellido;
	private int sueldo;
	private int id;
	
	
	public String getDni() {
		return dni;
	}
	public String getNombre() {
		return nombre;
	}
	public String getApellido() {
		return apellido;
	}
	public int getSueldo() {
		return sueldo;
	}
	public int getId() {
		return id;
	}
	
}
